{
  "tasks": {
    "start": "deno task bundle && DECO_SITE_NAME=lojastopmoveis deno run -A --unstable --watch=static/sw.js,tailwind.css,sections/,functions/,loaders/,actions/,workflows/,accounts/ dev.ts",
    "gen": "deno run -A dev.ts --gen-only",
    "component": "deno eval 'import \"deco/scripts/component.ts\"'",
    "release": "deno eval 'import \"deco/scripts/release.ts\"'",
    "update": "deno eval 'import \"deco/scripts/update.ts\"'",
    "check": "deno fmt && deno lint && deno check dev.ts main.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "install": "deno eval 'import \"deco/scripts/apps/install.ts\"'",
    "uninstall": "deno eval 'import \"deco/scripts/apps/uninstall.ts\"'",
    "bundle": "deno eval 'import \"deco/scripts/apps/bundle.ts\"' deco-sites/lojastopmoveis"
  },
  "githooks": { "pre-commit": "check" },
  "exclude": ["node_modules", "static/", "README.md", "**/_fresh/*"],
  "nodeModulesDir": true,
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "imports": {
    "deco-sites/lojastopmoveis/": "./",
    "$store/": "./",
    "deco-sites/std/": "https://cdn.jsdelivr.net/gh/deco-sites/std@1.26.6/",
    "$live/": "https://cdn.jsdelivr.net/gh/deco-cx/deco@1.75.2/",
    "deco/": "https://cdn.jsdelivr.net/gh/deco-cx/deco@1.75.2/",
    "$fresh/": "https://deno.land/x/fresh@1.6.8/",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "std/": "https://deno.land/std@0.190.0/",
    "partytown/": "https://denopkg.com/deco-cx/partytown@0.4.8/",
    "daisyui": "npm:daisyui@3.0.3",
    "apps/": "https://denopkg.com/deco-cx/apps@0.47.8/"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } }
}
